{
  "expo": {
    "name": "VCA_Project",
    "slug": "VCA_Project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/splash/splash0.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/splash/splash0.png",
      "resizeMode": "cover"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/splash/splash0.png",
        "backgroundColor": "#fff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photo",
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ]
    ]
  }
}
